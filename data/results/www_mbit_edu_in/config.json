{
    "project_name": "college_crawler_www.mbit.edu.in",
    "goal": "Extract admission, placement, and internship information",
    "seed_urls": [
        "https://www.mbit.edu.in"
    ],
    "keywords": [
        "admission",
        "placement",
        "internship",
        "eligibility",
        "fee",
        "cutoff",
        "entrance exam",
        "scholarship",
        "placed",
        "packages",
        "recruiter"
    ],
    "allowed_domains": [
        "www.mbit.edu.in"
    ],
    "max_depth": 3,
    "max_pages_per_domain": 50,
    "crawl_frequency": "daily",
    "custom_frequency_minutes": 1440,
    "respect_robots_txt": false,
    "user_agent": "SmartWebCrawler/1.0",
    "extract_full_text": true,
    "min_content_length": 100,
    "filter_advertisements": true,
    "use_nlp": true,
    "use_ml_classification": false,
    "confidence_threshold": 0.7,
    "output_directory": "d:\\web_crawler\\data\\results\\www_mbit_edu_in",
    "output_format": "json",
    "save_html": false,
    "parallel_crawlers": 1,
    "request_timeout": 30,
    "request_delay": 1.0,
    "enable_notifications": false,
    "notification_email": ""
}